<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @artifact.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @artifact.description %>
</p>

<p>
  <strong>Longitude:</strong>
  <%= @artifact.longitude %>
</p>

<p>
  <strong>Latitude:</strong>
  <%= @artifact.latitude %>
</p>

<p>
  <strong>Filepath:</strong>
  <%= @artifact.filepath %>
</p>

<button type="button" class="btn btn-primary"><%= link_to "Artifact's Tags", "/artifacts/#{@artifact.id}/tags"  %></button>

<button type="button" class="btn btn-primary"><%= link_to "Artifact's Users", "/artifacts/#{@artifact.id}/users"  %></button>

<div class="btn-group" role="group" aria-label="...">
  <button type="button" class="btn btn-default"><%= link_to 'Edit', edit_artifact_path(@artifact) %></button>
  <button type="button" class="btn btn-default"><%= link_to 'Back', :back %></button>
</div>

